{
    "appService.preDeployTask": "publish-release",
    "appService.deploySubpath": "bin/Release/net472/publish",
    "azureFunctions.deploySubpath": "bin/Release/net8.0/publish",
    "azureFunctions.projectLanguage": "C#",
    "azureFunctions.projectRuntime": "~4",
    "debug.internalConsoleOptions": "neverOpen",
    "azureFunctions.preDeployTask": "publish (functions)",
    "files.autoSave": "onWindowChange",
    "jupyter.allowUnauthorizedRemoteConnection": true,
    "aws.telemetry": false,
    "editor.accessibilityPageSize": 10000,
    "editor.accessibilitySupport": "off",
    "anycode.language.features": {
        "completions": false,
        "references": false,
        "highlights": false,
        "outline": false,
        "workspaceSymbols": false
    }
}